{
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/var/log/pool/metawriter",
        "flag": "debug"
    },
    "alert": {
        "host": "192.168.1.1",
        "addr": "192.168.1.1:10000"
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:3000",
            "udp@0.0.0.0:3000"
        ],
        "max_pkg_size": 1048576
    },
    "monitor": {
        "bind": "tcp@0.0.0.0:3100",
        "max_pkg_size": 10240
    },
    "redis": {
        "host": "127.0.0.1",
        "port": 6379
    },
    "cli": "tcp@127.0.0.1:4001",
    "dup_timeout": 3600,
    "trust_file": "trust.json"
}
